<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 <mapper namespace="com.yesgaori.spring.test.jsp.repository.RealtorRepository">
 	
 	<insert id="addRealtorRepository" parameterType="map">
 		
 		INSERT INTO
 			`realtor`
			(
 			`office`
 			, `phoneNumber`
 			, `address`
 			, `grade`
			)
 		VALUE
			(
 			#{office}
 			, #{phoneNumber}
 			, #{address}
 			, #{grade}
			)
 	</insert>
 	
 	<select id="selectRealtor" resultType="com.yesgaori.spring.test.jsp.domain.Realtor">
 		
 		SELECT
 			`id`
 			, `office`
 			, `phoneNumber`
 			, `address`
 			, `grade`
 			, `createdAt`
 			, `uppdatedAt`
 		FROM
 			`realtor`
 		ORDER BY
 			`id` DESC
 		LIMIT
 			1
 		
 		
 	</select>
 	
 
 </mapper>